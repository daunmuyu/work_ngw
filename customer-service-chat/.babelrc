{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "chrome": 52,
        "browserslist": ["> 1%", "last 2 versions",  "safari >= 7", "not ie <= 8"]
      }
    }]
  ],
  "plugins": [
    "transform-runtime",
    "transform-object-rest-spread"
  ],
  "env": {
    "test": {
      "presets": ["env"],
      "plugins": ["istanbul"]
    }
  }
}
